{
    "private": false,
    "version": "1.9.0",
    "name": "@polymedia/profile-sdk",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://profile.polymedia.app",
    "description": "An SDK to interact with Polymedia Profile objects on the Sui network",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-profile"
    },
    "keywords": [
        "polymedia",
        "sui",
        "profile"
    ],
    "scripts": {
        "lint": "eslint ./src --ext .ts,.tsx",
        "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "prepublishOnly": "pnpm build",
        "upgrade-sui": "pnpm i @mysten/bcs@latest @mysten/sui.js@latest @mysten/wallet-kit-core@latest"
    },
    "dependencies": {
        "@mysten/bcs": "^0.9.0",
        "@mysten/sui.js": "^0.48.0",
        "@mysten/wallet-kit-core": "^0.6.14"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "eslint-plugin-deprecation": "^1.5.0",
        "typescript": "^5.3.3"
    },
    "type": "module",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "types": "./dist/cjs/index.d.ts",
    "main": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./dist/cjs/index.d.ts",
            "main": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    }
}
