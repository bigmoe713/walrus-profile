{
    "name": "@polymedia/profile-sdk",
    "description": "An SDK to interact with Polymedia Profile objects on the Sui network",
    "version": "1.4.1",
    "license": "Apache-2.0",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://profile.polymedia.app",
